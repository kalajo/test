SELECT EXTRACTVALUE(
         t.xml_column,
         '/tns:DetermineContractIssuanceRequest/tns:Participant/participant:Institution/institution:Name',
         'xmlns:tns="http://rules.tiaa.org/brms-contract-issuance-v1.0/types"
          xmlns:participant="http://cbl.tiaa.org/participant/v1"
          xmlns:institution="http://cbl.tiaa.org/institution/v1"'
       ) AS institution_name
FROM your_table t;
