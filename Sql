SELECT x.Id
FROM your_table,
     XMLTABLE(
        XMLNAMESPACES(
          'http://cbl.tiaa.org/document/vi' AS "ns13",
          'http://cbl.tian.org/documenttvpenumber/vi' AS "ns14",
          'http://cbl.tlaa.org/electronicdocumentlibrary/v1' AS "ns15"
        ),
        '/ns2:DetermineContractIssuanceResponse/ns2:Documents/ns13:Document/ns13:DocumentTypeNumber/ns14:ElectronicDocumentLibrary/ns15:Id'
        PASSING XMLTYPE(xml_clob_column)
        COLUMNS
          Id VARCHAR2(50) PATH '.'
     ) x
WHERE x.Id = 'CRFST2';
